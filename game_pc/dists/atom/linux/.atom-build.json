{
    "name": "Free Pascal compile debug",
    "cmd": "{PROJECT_PATH}/dists/atom/linux/compile.sh",
    "args": ["{PROJECT_PATH}", "compile-debug"],
    "cwd": "{PROJECT_PATH}",
    "sh": true,
    "atomCommandName": "worldparty:compile-debug",
    "errorMatch": ["(?<file>.+)\\((?<line>\\d+)\\,(?<col>\\d+)\\) (?<message>.+)"],
    "keymap": "f9",
    "targets": {
        "Free Pascal compile": {
            "cmd": "{PROJECT_PATH}/dists/atom/linux/compile.sh",
            "args": ["{PROJECT_PATH}", "compile"],
            "cwd": "{PROJECT_PATH}",
            "sh": true,
            "atomCommandName": "worldparty:compile",
            "errorMatch": ["(?<file>.+)\\((?<line>\\d+)\\,(?<col>\\d+)\\) (?<message>.+)"],
            "keymap": "ctrl-f9"
        },
        "Free Pascal compile debug and execute": {
            "cmd": "{PROJECT_PATH}/dists/atom/linux/compile.sh",
            "args": ["{PROJECT_PATH}", "compile-debug-execute"],
            "cwd": "{PROJECT_PATH}",
            "sh": true,
            "atomCommandName": "worldparty:compile-debug-execute",
            "errorMatch": ["(?<file>.+)\\((?<line>\\d+)\\,(?<col>\\d+)\\) (?<message>.+)"],
            "keymap": "f10"
        },
        "Free Pascal compile and execute": {
            "cmd": "{PROJECT_PATH}/dists/atom/linux/compile.sh",
            "args": ["{PROJECT_PATH}", "compile-execute"],
            "cwd": "{PROJECT_PATH}",
            "sh": true,
            "atomCommandName": "worldparty:compile-execute",
            "errorMatch": ["(?<file>.+)\\((?<line>\\d+)\\,(?<col>\\d+)\\) (?<message>.+)"],
            "keymap": "ctrl-f10"
        },
        "Free Pascal execute debug without compile": {
            "cmd": "{PROJECT_PATH}/dists/atom/linux/compile.sh",
            "args": ["{PROJECT_PATH}", "execute-debug"],
            "cwd": "{PROJECT_PATH}",
            "sh": true,
            "atomCommandName": "worldparty:execute-debug",
            "errorMatch": ["(?<file>.+)\\((?<line>\\d+)\\,(?<col>\\d+)\\) (?<message>.+)"],
            "keymap": "f11"
        },
        "Free Pascal execute without compile": {
            "cmd": "{PROJECT_PATH}/dists/atom/linux/compile.sh",
            "args": ["{PROJECT_PATH}", "execute"],
            "cwd": "{PROJECT_PATH}",
            "sh": true,
            "atomCommandName": "worldparty:execute",
            "errorMatch": ["(?<file>.+)\\((?<line>\\d+)\\,(?<col>\\d+)\\) (?<message>.+)"],
            "keymap": "ctrl-f11"
        }
    }
}
